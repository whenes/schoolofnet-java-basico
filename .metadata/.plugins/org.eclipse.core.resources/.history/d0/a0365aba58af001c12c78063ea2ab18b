package com.schoolofnet.learning;

public class HelloWorld {
	public static void main(String[] args) {
		
		//primitivos
		int myInt = 0;
		char myChar = 'C';
		byte myByte;
		float myFloat = 0;
		double myDouble = 0.0;
		
		//wrappers
		Integer myIntW = null;
		Character myCharacterW = 'C';
		Byte myByteW = null;
		Float myFloatW = null;
		Double myDoubleW = null;
		String myStringW = "STR";
		
		System.out.println("Hello world from School of Net!");
	}
}
